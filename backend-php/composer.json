{
    "name": "kangli/digital-backend",
    "description": "康立数字微信服务号后端API",
    "type": "project",
    "keywords": [
        "thinkphp",
        "api",
        "backend"
    ],
    "license": "Apache-2.0",
    "require": {
        "php": ">=7.2.5",
        "topthink/framework": "^6.0",
        "topthink/think-orm": "^2.0",
        "topthink/think-view": "^1.0",
        "firebase/php-jwt": "^6.0",
        "aliyuncs/oss-sdk-php": "^2.6",
        "predis/predis": "^2.0"
    },
    "require-dev": {
        "symfony/var-dumper": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "app\\": "app/"
        },
        "files": [
            "app/common.php"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "scripts": {
        "post-autoload-dump": [
            "@php think service:discover",
            "@php think vendor:publish"
        ]
    }
}

